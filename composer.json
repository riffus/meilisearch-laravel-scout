{
    "name": "riffus/meilisearch-laravel-scout",
    "description": "Laravel Scout custom engine for MeiliSearch",
    "keywords": ["meilisearch", "laravel", "scout", "search"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Massart",
            "email": "hello@jordan-massart.be"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel/scout": "^8.0",
        "meilisearch/meilisearch-php": "^0.9|^0.10"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Meilisearch\\Scout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meilisearch\\Scout\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meilisearch\\Scout\\MeilisearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
